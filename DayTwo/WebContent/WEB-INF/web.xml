<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" version="4.0">
  <display-name>DayTwo</display-name>
  <welcome-file-list> 
    <welcome-file>main.jsp</welcome-file>
  </welcome-file-list>
  
   <servlet>
  	<servlet-name>Board</servlet-name>
  	<servlet-class>com.controller.BoardServlet</servlet-class>	
  </servlet>
  <servlet>
  	<servlet-name>Member</servlet-name>
  	<servlet-class>com.controller.MemberServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>MemberProfile</servlet-name>
  	<servlet-class>com.user.MemberProfileServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>Board</servlet-name>
  	<url-pattern>/board.do</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>Member</servlet-name>
  	<url-pattern>/member.do</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>MemberProfile</servlet-name>
  	<url-pattern>/profile.do</url-pattern>
  </servlet-mapping>
  
  
</web-app>